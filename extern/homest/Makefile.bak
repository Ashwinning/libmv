#
# Unix/Linux GCC Makefile for homography estimation
#

CC=gcc
LEVMAR_PATH=/home/lourakis/levmar-src/levmar-2.2/
INCLUDES=-I$(LEVMAR_PATH)
CFLAGS=$(INCLUDES) -O3 -funroll-loops -Wall #-pg
LAPACKLIBS_PATH=/usr/local/lib # WHEN USING LAPACK, CHANGE THIS TO WHERE YOUR COMPILED LIBS ARE!
LDFLAGS=-L$(LEVMAR_PATH) -L$(LAPACKLIBS_PATH)

HOMEST_DEMO_OBJS=calc_2Dhomog_coeffs.o homest_demo.o homest.o lqs.o min_Ax_norm.o norm.o buckets.o
HOMEST_DEMO_SRCS=calc_2Dhomog_coeffs.c homest_demo.c homest.c lqs.c min_Ax_norm.c norm.c buckets.c
AR=ar
RANLIB=ranlib
RM=rm -f
MAKE=make
MAPLE=maple

LAPACKLIBS=-llapack -lblas -lf2c

homest_demo: $(HOMEST_DEMO_OBJS)
	$(CC) $(LDFLAGS) $(HOMEST_DEMO_OBJS) -o homest_demo -llevmar $(LAPACKLIBS) -lm 

calc_2Dhomog_coeffs.c: homo2D.mpl
	$(MAPLE) <  $<

clean:
	-$(RM) $(HOMEST_DEMO_OBJS) gmon.out

depend:
	makedepend $(INCLUDES) -f Makefile $(HOMEST_DEMO_SRCS)

