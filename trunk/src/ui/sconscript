Import('options')
options['tools'] = ['default','qt4']
options['toolpath'] += ['.']

env = Environment(**options)
env.EnableQt4Modules(["QtGui","QtCore","QtSvg"])
env.Program(
    target = 'imageviewer',
    source = ['main.cc',
              'imageviewer.cc',
              'scrubber.cc',
              '#bin-dbg/libmv/image/libimage.a'],)
